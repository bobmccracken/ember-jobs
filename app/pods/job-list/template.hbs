

<ul class="list-group">
{{#each jobs as |job|}}
   <li  >

    <div class='col-md-3 col-md-offset-3' >
      {{#link-to 'job' job}}
        {{job.title}}
      {{/link-to}}
      at {{job.company.name}}
    </div>
    <div class='col-md-3'>
      {{job.location.city}}
    </div>
    <div class='col-md-3'>
      Posted: {{job.postedAt}}
    
    
      {{#link-to 'job' job}}
      
        <span class='glyphicon glyphicon-triangle-right' aria-hidden="true"></span>
      
      {{/link-to}}
    </div>

  
  </li>
{{/each}}
</ul>





